{
  "name": "custom-block-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 4000",
    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "blocks": [
    {
      "type": "file",
      "title": "Example File Block",
      "description": "A basic file block",
      "entry": "/src/blocks/example-file-block/index.tsx",
      "extensions": [
        "*"
      ]
    },
    {
      "type": "folder",
      "title": "Example Folder Block",
      "description": "A basic folder block",
      "entry": "/src/blocks/example-folder-block/index.tsx",
      "extensions": [
        "*"
      ]
    }
  ],
  "dependencies": {
    "@githubnext/utils": "^0.9.18",
    "@loadable/component": "^5.15.0",
    "@octokit/rest": "^18.12.0",
    "git-url-parse": "^11.6.0",
    "lodash": "^4.17.21",
    "parse-static-imports": "^1.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-error-boundary": "^3.1.4",
    "react-query": "^3.32.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@types/git-url-parse": "^9.0.1",
    "@types/lodash": "^4.14.176",
    "@types/parse-github-url": "^1.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  }
}
